{
  "name": "nanogram",
  "version": "0.0.2",
  "description": "A parsing library with nothing special.",
  "main": "src/index.js",
  "scripts": {
    "check": "npm run lint:check && npm run test",
    "cover": "nyc --exit --reporter=lcov --reporter=text --per-file npm run test",
    "lint:check": "standard",
    "lint": "standard --fix",
    "test:dev": "mocha -ui tdd --recursive --watch test",
    "test": "mocha -ui tdd --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreventuravale/nanogram.git"
  },
  "standard": {
    "globals": [
      "setup",
      "suite",
      "test"
    ]
  },
  "keywords": [
    "parser",
    "parsers"
  ],
  "author": "andreventuravale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andreventuravale/nanogram/issues"
  },
  "homepage": "https://github.com/andreventuravale/nanogram#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "clipboardy": "^2.1.0",
    "coveralls": "^3.0.4",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "standard": "^13.1.0",
    "testdouble": "^3.12.2"
  }
}
